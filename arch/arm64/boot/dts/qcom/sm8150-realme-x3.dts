&spi17 {
	status = "okay";

	touchscreen@0 {
		compatible = "novatek,nt36xxx-spi";
		reg = <0>;

		spi-max-frequency = <15000000>;

		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&ts_int_active &ts_reset_active>;
		pinctrl-1 = <&ts_int_suspend &ts_reset_suspend>;

		reset-gpio = <&tlmm 54 GPIO_ACTIVE_HIGH>;
		irq-gpio = <&tlmm 122 IRQ_TYPE_EDGE_FALLING>;

		firmware-name = "novatek/novatek_nt36672c_fw.bin";
	};
};

&tlmm {
	gpio-reserved-ranges = <0 4>, <126 4>;

	ts_reset_active: ts-reset-active-state {
		pins = "gpio54";
		function = "gpio";
		drive-strength = <16>;
		bias-pull-up;
	};

	ts_reset_suspend: ts-reset-suspend-state {
		pins = "gpio54";
		function = "gpio";
		drive-strength = <16>;
		bias-disable;
		output-high;
	};

	ts_int_active: ts-int-active-state {
		pins = "gpio122";
		function = "gpio";
		drive-strength = <16>;
		bias-pull-up;
	};

	ts_int_suspend: ts-int-suspend-state {
		pins = "gpio122";
		function = "gpio";
		drive-strength = <16>;
		input-enable;
		bias-disable;
	};

	quat_mi2s_active: quat_mi2s_active_state {
		pins = "gpio137", "gpio138";
		function = "qua_mi2s";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	quat_mi2s_mclk_active: quat_mi2s_mclk_active {
		pins = "gpio136";
		function = "qua_mi2s";
		drive-strength = <8>;
		bias-disable;
	};

	quat_mi2s_sd0_active: quat_mi2s_sd0_active {
		pins = "gpio139";
		function = "qua_mi2s";
		drive-strength = <8>;
		bias-disable;
	};

	quat_mi2s_sd1_active: quat_mi2s_sd1_active {
		pins = "gpio140";
		function = "qua_mi2s";
		drive-strength = <8>;
		bias-disable;
	};

	qup_uart13_sleep: qup-uart13-sleep {
		pinmux {
			pins = "gpio43", "gpio44",
			       "gpio45", "gpio46";
			function = "gpio";
		};

		pinconf-cts {
			/*
			 * Configure a pull-down on CTS to match the pull of
			 * the Bluetooth module.
			 */
			pins = "gpio43";
			bias-pull-down;
		};

		pinconf-rts {
			/*
			 * Configure pull-down on RTS. As RTS is active low
			 * signal, pull it low to indicate the BT SoC that it
			 * can wakeup the system anytime from suspend state by
			 * pulling RX low (by sending wakeup bytes).
			 */
			 pins = "gpio44";
			 bias-pull-down;
		};

		pinconf-tx {
			/*
			 * Configure pull-up on TX when it isn't actively driven
			 * to prevent BT SoC from receiving garbage during sleep.
			 */
			pins = "gpio45";
			bias-pull-up;
		};

		pinconf-rx {
			/*
			 * Configure a pull-up on RX. This is needed to avoid
			 * garbage data when the TX pin of the Bluetooth module
			 * is floating which may cause spurious wakeups.
			 */
			pins = "gpio46";
			bias-pull-up;
		};
	};
};

